{
  "name": "helia-playground",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Helia playground for browser",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./.cache ./node_modules/.vite",
    "generate:proto:dcutr": "protons ./src/dcutr/message.proto",
    "build": "npm run generate:proto:dcutr && vite build",
    "serve": "vite dev --port 8888",
    "start": "npm run serve",
    "test": "npm run build && playwright test tests",
    "update-deps": "ncu",
    "postinstall": "patch-package"
  },
  "browserslist": "last 1 Chrome version",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@playwright/test": "^1.36.0",
    "@types/debug": "^4.1.8",
    "@types/react": "^18.2.18",
    "npm-check-updates": "^16.10.15",
    "patch-package": "^8.0.0",
    "playwright": "^1.36.0",
    "process": "^0.11.10",
    "protons": "^7.0.5",
    "rimraf": "^5.0.1",
    "test-util-ipfs-example": "^1.0.2",
    "util": "^0.12.5",
    "vite": "^4.4.3"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^10.0.0",
    "@chainsafe/libp2p-noise": "^13.0.0",
    "@chainsafe/libp2p-yamux": "^5.0.0",
    "@helia/unixfs": "^1.4.1",
    "@libp2p/bootstrap": "^9.0.2",
    "@libp2p/kad-dht": "^10.0.2",
    "@libp2p/mplex": "^9.0.2",
    "@libp2p/webrtc": "^3.1.3",
    "@libp2p/websockets": "^7.0.2",
    "@multiformats/multiaddr-matcher": "^1.0.1",
    "blockstore-core": "^4.2.0",
    "blockstore-level": "^1.1.1",
    "datastore-core": "^9.2.0",
    "datastore-level": "^10.1.2",
    "debug": "^4.3.4",
    "delay": "^6.0.0",
    "helia": "^1.3.12",
    "libp2p": "0.46.3",
    "protons-runtime": "^5.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "overrides": {
    "libp2p": "0.46.3"
  }
}
